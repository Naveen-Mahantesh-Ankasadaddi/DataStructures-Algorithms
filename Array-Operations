//1.Traversing
#include <stdio.h>

int main(){
    int arr[100],i,num;
    printf("Enter size of array: ");
    scanf("%d",&num);
    printf("Enter %d elements in the array : ",num);
    for(i=0;i<num;i++){
        scanf("%d", &arr[i]);
    }
    printf("\nElements in array are: ");
    for(i=0;i<num;i++){
        printf("%d \n", arr[i]);
    }
    return 0;
}

Output:
                    Enter size of array: 10
                    Enter 10 elements in the array : 10
                    20
                    3
                    4
                    5
                    6
                    7
                    8
                    9
                    0
                    Elements in array are: 10
                    20
                    3
                    4
                    5
                    6
                    7
                    8
                    9
                    0

//2.insertion
 #include<stdio.h>
int main(){
    int arr[5]={10,20,30,40,50},i,item=9;
    int n=5,k=5,j=n;
    printf("The original array elements are:\n");
    for(i=0;i<5;i++){
        printf("arr[%d] is %d\n",i,arr[i]);
    }
    printf("Enter the number to insert:\n");
    scanf("%d",&item);

    //printf("Enter the position to insert:\n");
    //scanf("%d",&k);

    n=n+1;
    while(j>=k){
        arr[j+1]=arr[j];
        j=j-1;
    }
    arr[k]=item;

     printf("The array elements  after insertion are:\n");
    for(i=0;i<=5;i++){
        printf("arr[%d] is %d\n",i,arr[i]);
    }
    return 0;
}
Output:
                    The original array elements are:
                    arr[0] is 10
                    arr[1] is 20
                    arr[2] is 30
                    arr[3] is 40
                    arr[4] is 50
                    Enter the number to insert:
                    99
                    The array elements  after insertion are:
                    arr[0] is 10
                    arr[1] is 20
                    arr[2] is 30
                    arr[3] is 40
                    arr[4] is 50
                    arr[5] is 99
