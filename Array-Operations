//1.Traversing
#include <stdio.h>

int main(){
    int arr[100],i,num;
    printf("Enter size of array: ");
    scanf("%d",&num);
    printf("Enter %d elements in the array : ",num);
    for(i=0;i<num;i++){
        scanf("%d", &arr[i]);
    }
    printf("\nElements in array are: ");
    for(i=0;i<num;i++){
        printf("%d \n", arr[i]);
    }
    return 0;
}

Output:
Enter size of array: 10
Enter 10 elements in the array : 10
20
3
4
5
6
7
8
9
0
Elements in array are: 10
20
3
4
5
6
7
8
9
0

